{
    "input": [
        {
            "type": "text",
            "action": "add",
            "args": {
                "inputDir": "${RULESET_SOURCE_GEOIP_DIR}"
            }
        },
        {
            "type": "v2rayGeoIPDat",
            "action": "add",
            "args": {
                "uri": "${RULESET_GEOIP_PATH}"
            }
        }
    ],
    "output": [
        {
            "type": "v2rayGeoIPDat",
            "action": "output",
            "args": {
                "outputDir": "${CURRENT_DIR}",
                "outputName": "${IP_FULL_FILE_NAME}",
                "wantedList": ["ru", "private", "home"]
            }
        }
    ]
}